{
  "name": "babel-plugin-rename-umd-globals",
  "version": "0.0.1",
  "description": "Renames the module's global variable and adds optional global aliases",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "jsnext:main": "src/index.js",
  "scripts": {
    "test": "mocha --ui tdd",
    "autotest": "mocha --ui tdd --watch",
    "build": "npm run lint && npm test && npm run babel",
    "babel": "babel src -d lib",
    "lint": "eslint src test",
    "cover": "nyc --require babel-core/register --all mocha"
  },
  "nyc": {
    "include": [
      "src"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamonkko/babel-plugin-rename-umd-globals.git"
  },
  "keywords": [
    "global",
    "module",
    "babel",
    "rename",
    "umd",
    "name",
    "globals",
    "export"
  ],
  "author": "Jarkko Mönkkönen <jamonkko@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamonkko/babel-plugin-rename-umd-globals/issues"
  },
  "homepage": "https://github.com/jamonkko/babel-plugin-rename-umd-globals#readme",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-eslint": "^6.0.2",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "coveralls": "^2.11.9",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^8.0.0",
    "eslint-plugin-import": "^1.5.0",
    "estraverse-fb": "^1.3.1",
    "mocha": "^2.4.5",
    "nyc": "^6.4.4"
  },
  "dependencies": {
    "babel-plugin-rename-assigned-properties": "0.0.5"
  }
}
